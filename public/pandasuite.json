    "markers": [
      {
        "hideSystemId": true,
        "enableSource": true,
        "params": [
          {
            "id": "latlng",
            "name": "Center",
            "locale_name": {
              "fr_FR": "Centre"
            },
            "type": "Coord",
            "value": {
              "lat": 48.861,
              "lng": 2.344
            },
            "bindable": true,
            "hidden": "marker.data.properties.type != 'marker' && marker.data.properties.type != 'circlemarker' && marker.data.properties.type != 'circle'"
